{
"manifest_version": 2,
    "name": "SafeSurf",
    "description": "A chrome extension to bleep out the bad stuff!",
    "version": "0.1",
    "browser_action": {
      "default_icon": "icon.png",
     "default_popup": "popup.html"
     },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "cleanup.js"
            ],
            "run_at": "document_end"
        }
    ]
}