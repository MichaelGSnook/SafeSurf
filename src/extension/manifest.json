{
"manifest_version": 2,
    "name": "SafeSurf",
    "description": "A chrome extension to bleep out the bad stuff!",
    "version": "0.1",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
     },
    "web_accessible_resources": [
        "lib/bad_words.txt"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "lib/jquery-2.2.2.min.js", "cleanup.js" 
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage"
    ],
}